{
  "name": "react-marrow",
  "version": "0.1.9",
  "description": "Skeleton loader for React",
  "keywords": [
    "typescript",
    "skeleton",
    "loader"
  ],
  "homepage": "https://github.com/dalhaan/marrow",
  "bugs": {
    "url": "https://github.com/dalhaan/marrow/issues"
  },
  "author": "Dallan Freemantle <dallan.npm@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalhaan/marrow.git"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "ci": "pnpm run build && pnpm run check-format && pnpm run check-exports && pnpm run test",
    "prepublishOnly": "npm run ci"
  },
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.8",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
